{
  "name": "kat/micro-orm",
  "type": "library",
  "description": "A micro orm for accessing databases",
  "minimum-stability": "stable",
  "license": "MIT",
  "authors": [
    {
      "name": "Khalid ATLASSI",
      "email": "khalid.atlassi@gmail.com"
    }
  ],
  "require": {
    "php": ">=7.4",
    "monolog/monolog": "^1.26",
    "ext-pdo": "*",
    "symfony/dotenv": "*"
  },
  "autoload": {
    "psr-4": {
      "Kat\\MicroORM\\": "src/"
    }
  },
  "require-dev": {
    "phpunit/phpunit": "^9.5.19"
  },
  "autoload-dev": {
    "psr-4": {
      "tests\\": "tests/"
    }
  },
  "config": {
    "platform": {
      "php": "7.4.0"
    }
  }
}